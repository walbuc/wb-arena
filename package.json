{
  "name": "wb-arena",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:server": "cross-env MOCKS=true tsx watch --clear-screen=false ./index.js",
    "dev:remix": "remix watch",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx ./index.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "postinstall": "patch-package --patch-dir other/patches"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@cld-apis/types": "^0.1.6",
    "@kentcdodds/md-temp": "^3.1.1",
    "@octokit/plugin-throttling": "^5.0.1",
    "@octokit/rest": "^19.0.7",
    "@prisma/client": "^4.9.0",
    "@reach/menu-button": "^0.18.0",
    "@remark-embedder/core": "^3.0.1",
    "@remark-embedder/transformer-oembed": "^3.0.0",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@remix-run/server-runtime": "^1.13.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^8.1.0",
    "cachified": "^3.1.0",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "date-fns": "^2.29.3",
    "esbuild": "^0.17.16",
    "express": "^4.18.2",
    "framer-motion": "^9.0.3",
    "fs-extra": "^11.1.1",
    "hast-util-to-string": "^2.0.0",
    "helmet": "^6.0.1",
    "isbot": "^3.6.5",
    "litefs-js": "^1.1.2",
    "lru-cache": "^7.14.1",
    "mdx-bundler": "^9.2.1",
    "morgan": "^1.10.0",
    "on-finished": "^2.4.1",
    "p-queue": "^7.3.4",
    "patch-package": "^6.5.1",
    "pm2": "^5.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-document": "^6.1.0",
    "rehype-format": "^4.0.1",
    "rehype-parse": "^8.0.4",
    "rehype-stringify": "^9.0.3",
    "remark-autolink-headings": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-slug": "^7.0.1",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.4",
    "@types/eslint": "^8.4.10",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "@types/on-finished": "^2.3.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.2",
    "autoprefixer": "^10.4.13",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.11.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "happy-dom": "^8.1.4",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "prisma": "^4.9.0",
    "start-server-and-test": "^1.15.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsx": "^3.12.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.27.2"
  },
  "engines": {
    "node": "18",
    "npm": "8"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
